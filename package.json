{
  "name": "unocss-preset-alloc",
  "version": "0.22.8",
  "description": "A fork of @unocss/preset-mini",
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "author": "Alec Larson",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./theme": {
      "require": "./dist/theme.js",
      "import": "./dist/theme.mjs",
      "types": "./dist/theme.d.ts"
    },
    "./variants": {
      "require": "./dist/variants.js",
      "import": "./dist/variants.mjs",
      "types": "./dist/variants.d.ts"
    },
    "./rules": {
      "require": "./dist/rules.js",
      "import": "./dist/rules.mjs",
      "types": "./dist/rules.d.ts"
    },
    "./colors": {
      "require": "./dist/colors.js",
      "import": "./dist/colors.mjs",
      "types": "./dist/colors.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.css"
  ],
  "scripts": {
    "dev": "tsup --sourcemap --watch",
    "build": "tsup --clean"
  },
  "dependencies": {
    "@unocss/core": "^0.22.7"
  },
  "devDependencies": {
    "tsup": "^6.0.1"
  }
}
